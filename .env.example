# Core database and queue
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=copil

REDIS_HOST=localhost
REDIS_PORT=6379

# Web origin for CORS (comma separated)
WEB_ORIGIN=http://localhost:3000

# API auth
JWT_SECRET=dev-secret

# Optional Privy verification (when using Privy-issued JWTs)
# PRIVY_APP_ID=
# PRIVY_PUBLIC_KEY_PEM=
# PRIVY_JWKS_ENDPOINT=

# Chain abstraction & quotes
ONEBALANCE_API_KEY=

# LLM provider (hidden from users)
# Set either OPENAI_API_KEY or GROQ_API_KEY
# Optionally select provider via LLM_PROVIDER=groq|openai
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o
GROQ_API_KEY=
GROQ_MODEL=llama-3.1-70b-versatile
CHAT_ENABLED=true

# RPC endpoints (set what you plan to execute on)
RPC_URL_ETHEREUM=
RPC_URL_BASE=
RPC_URL_ARBITRUM=
RPC_URL_LINEA=
RPC_URL_OPTIMISM=
RPC_URL_POLYGON=
RPC_URL_BSC=
RPC_URL_AVALANCHE=
RPC_URL_SEI=
RPC_URL_SOLANA=

# ERC-4337 / Pimlico (used by transaction-executor)
PIMLICO_API_KEY=
# Optional paymaster
PAYMASTER_ENABLED=false
# PIMLICO_PAYMASTER_API_KEY=

# Transaction executor health/metrics server
TX_EXECUTOR_PORT=3005

# Strategy evaluator â†’ API internal token
INTERNAL_API_TOKEN=
API_SERVICE_URL=http://localhost:4311

# Data ingestor
DEX_SCREENER_API_URL=https://api.dexscreener.com/latest/dex
HL_INGEST_ENABLED=true
HL_INGEST_SYMBOLS=BTC,ETH
SOL_INGEST_ENABLED=true
SOL_INGEST_MINTS=

# Axelar/Sei bridge (optional)
SEI_BRIDGE_ENABLED=false
# AXELAR_GATEWAY_ADDRESS_BASE=
# AXELAR_SEI_CHAIN_NAME=sei
# AXELAR_TOKEN_SYMBOL_USDC=aUSDC

# Session keys (examples)
