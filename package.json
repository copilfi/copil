{
  "name": "copil",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "e2e:smoke": "node scripts/e2e-smoke.mjs",
    "migrate:vault": "npx tsx scripts/migrate-keys-to-vault.ts",
    "migrate:vault:dry-run": "DRY_RUN=true npx tsx scripts/migrate-keys-to-vault.ts"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "web"
  ],
  "dependencies": {
    "@aws-sdk/client-kms": "^3.932.0",
    "merkletreejs": "^0.6.0"
  }
}
